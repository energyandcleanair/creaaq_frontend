{
  "name": "creaaq_frontend",
  "version": "0.5.2",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "serve:build": "npx serve -s dist -l 8080",
    "test": "npm run test:unit",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "pretty": "prettier --write \"./**/*.{ts,js,vue,scss,json,md}\"",
    "prod:create-env": "printenv > .env.production.local",
    "check:master": "if git branch --show-current | grep ^master$ > /dev/null; then echo 'Git branch: master'; else echo 'Not in the master branch' 1>&2 && exit 1; fi",
    "bump": "npm run check:master && npx bump prompt --tag 'v%s' --commit 'v%s' package.json package-lock.json",
    "patch": "npm run check:master && npx bump patch --tag 'v%s' --commit 'v%s' package.json package-lock.json",
    "preversion": "echo 'Lint & test before bump...' && npm run lint && npm run test",
    "deploy": "git push origin master --tags"
  },
  "dependencies": {
    "@gtm-support/vue2-gtm": "^1.2.1",
