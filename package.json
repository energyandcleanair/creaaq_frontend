    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.orderby": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqby": "^4.7.0",
    "moment": "^2.29.1",
    "register-service-worker": "^1.7.1",
    "tslib": "^2.2.0",
    "vue": "^2.6.11",
    "vue-axios": "^3.2.4",
    "vue-class-component": "^7.2.3",
    "vue-country-flag": "^2.1.1",
